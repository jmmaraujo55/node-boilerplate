  ],
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.6.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.10.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.2",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13"
  }
}
